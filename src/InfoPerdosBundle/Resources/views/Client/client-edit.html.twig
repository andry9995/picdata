                        </div>
                    </div>


                    <div class="form-group">
                        <label class="col-lg-6 control-label">Adresse IP</label>
                        <div class="col-lg-6">
                            <input type="text" placeholder="Adresse IP" class="form-control"
                                   name="adresse_ip" id="js_adresse_ip"  data-field="Ip" value="{{ (clientLogiciel is null) ? '' : (clientLogiciel.ip is null) ? '' : clientLogiciel.ip }}">
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-lg-6 control-label">Implantation</label>
                        <div class="col-lg-6">
                            <input type="text" placeholder="Implantation" class="form-control"
                                   name="implantation" id="js_implantation"  data-field="Implantation" value="{{ (clientLogiciel is null) ? '' : (clientLogiciel.implantation is null) ? '' : clientLogiciel.implantation }}">
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-lg-6 control-label">Login</label>
                        <div class="col-lg-6">
                            <input type="text" placeholder="Login" class="form-control"
                                   name="login" id="js_login" data-field="Login" value="{{ (clientLogiciel is null) ? '' : (clientLogiciel.login is null) ? '' : clientLogiciel.login }}">
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-lg-6 control-label">Mot de passe</label>
                        <div class="col-lg-6">
                            <input type="text" placeholder="Mot de passe" class="form-control"
                                   name="password" id="js_password" data-field="Password" value="{{ (clientLogiciel is null) ? '' : (clientLogiciel.password is null) ? '' : clientLogiciel.password }}">
                        </div>
                    </div>
                </div>
            </div>










