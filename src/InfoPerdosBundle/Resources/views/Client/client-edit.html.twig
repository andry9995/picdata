                                {% set nomPrenom = nom ~ ' ; ' ~ prenom %}
                            {% endif %}
                            <input type="text" placeholder="Nom;Prénom" class="form-control" name="nom_secretaire"
                                   id="js_nom_sercretaire" value="{{ nomPrenom }}">
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-lg-6 control-label">Tel. portable</label>
                        <div class="col-lg-6">
                            <input type="text" placeholder="Tel. portable" class="form-control" name="tel_secretaire"
                                   id="js_tel_secretaire" value="{{ (secretaire is null) ? '' : (secretaire.telPortable is null) ? '' : secretaire.telPortable }}">
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-lg-6 control-label">Mail</label>
                        <div class="col-lg-6">
                            <input type="email" placeholder="Mail" class="form-control" name="mail_secretaire"
                                   id="js_mail_secretaire" value="{{ (secretaire is null) ? '' : (secretaire.email is null) ? '' : secretaire.email }}">
                        </div>
                    </div>


                </div>

            </div>





            <div class="ibox border-bottom">
                <div class="ibox-content">
                    <h2>Logiciel utilisé</h2>
                    <div class="form-group">
                        <label class="col-lg-6 control-label">Logiciel</label>
                        <div class="col-lg-6">

                            {% set logicielId  = (clientLogiciel is null) ? -1 : (clientLogiciel.logiciel is null) ? -1 : clientLogiciel.logiciel.id %}


