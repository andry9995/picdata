<div class="form-horizontal">
    <div class="form-group">
        <label class="col-lg-2 control-label" for="id_new_indicateur_tb">Domaine</label>
        <div class="col-lg-10">
            <select class="form-control" id="id_new_indicateur_tb" data-indicateur_tb="{{ indicateurTb.id|boost }}">
                {% for indicateurTbDomaine in indicateurTbDomaines %}
                    <option value="{{ indicateurTbDomaine.id|boost }}" {{ indicateurTb.indicateurTbDomaine.id == indicateurTbDomaine.id ? 'selected' : '' }}>
                        {{ indicateurTbDomaine.nom }}
                    </option>
                {% endfor %}
            </select>
        </div>
    </div>
    <div class="form-group">
        <div class="col-lg-12 text-right">
            <span class="btn btn-sm btn-white" id="id_tranferer_indicateur_tb">Tranf&eacute;rer</span>
        </div>
    </div>
</div>